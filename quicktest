#!/usr/bin/env elixir
Mix.start(); Code.require_file("mix.exs",__DIR__); Mix.Task.run("run",[])


defmodule TheModule do
  defrecordp :r_Unruly, name: "george", age: 123

  def go do
    a = r_Unruly()
    IO.inspect a
  end

end

TheModule.go


